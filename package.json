  },
  "homepage": "https://github.com/ogwurujohnson/Politico#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/helpers": "^7.3.1",
    "@babel/node": "^7.2.2",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
