{
  "name": "politico",
  "version": "1.0.0",
  "description": "Politico enables citizens give their mandate to politicians running for different government offices while building trust in the process through transparency.",
  "main": "./server/app.js",
  "scripts": {
    "start": "set NODE_ENV=production&& babel-node ./server/app.js",
    "start:dev": "set NODE_ENV=development&& babel-watch ./server/app.js",
    "test": "set NODE_ENV=test&& babel-node ./server/models/db.js createAllTables && nyc mocha  --timeout 15000  --require babel-register --require babel-polyfill ./server/test/* --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pg": "set NODE_ENV=development&& babel-node ./server/models/db.js dropAllTables && babel-node ./server/models/db.js createAllTables && babel-node ./server/models/db.js seedAllTables --exec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogwurujohnson/Politico.git"
  },
  "author": "Johnson Ogwuru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ogwurujohnson/Politico/issues"
  },
  "homepage": "https://github.com/ogwurujohnson/Politico#readme",
  "dependencies": {
    "@babel/node": "^7.2.2",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "path": "^0.12.7",
    "pg": "^7.8.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2"
  }
}
